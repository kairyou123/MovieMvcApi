@model List<MovieMvcApi.Models.MovieModel>
<div class="text-center">
    <h1 class="display-4">Index Movie</h1>
</div>
    <table class="table">
        <thead><th>Title</th>
        <th>Genre</th></thead>
    <tbody>
    @foreach(var movie in Model) {

            <tr>
                <td width="50%">@movie.Title</td>
                <td width="50%">@movie.Genre</td>
            </tr>
        
    }
    </tbody>
    </table>
